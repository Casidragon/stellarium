"     QTabBar::tab {\n"
"         background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 "
"\t#2a3b50, stop: 1 #19233c);\n"
"         border: 2px solid #348483;\n"
"         border-bottom-color: #348483; /* same as the pane color */\n"
"         border-top-left-radius: 4px;\n"
