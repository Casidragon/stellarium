rootLabelText = "Timer (script second since start): ";
statusLabelId = LabelMgr.labelScreen("", 50, 50, true, 16, "#ff0000");
var counter = 0;

while(1)
{
	status(counter++);
	core.wait(1);
}

function status(text)
{
	 LabelMgr.setLabelText(statusLabelId, rootLabelText + text);
}
